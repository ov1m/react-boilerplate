{
  "presets": [
    [
      "env",
      {
      "targets": {
        "browsers": ["last 2 versions"]
        },
        "modules": false,
        "loose": true
      }
    ],
    "react"
  ],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "env": {
      "production": {
        "plugins": ["transform-react-remove-prop-types"]
      }
    }
  }
}