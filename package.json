{
  "name": "react-webpack-boileplate",
  "version": "0.0.1",
  "description": "React Webpack boileplate",
  "main": "index.js",
  "repository": "https://github.com/ov1m/react-boilerplate.git",
  "author": "Ovidiu Mocan",
  "license": "MIT",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "webpack -p",
    "lint": "eslint ./src",
    "predev": "rm -rf dist",
    "dev": "webpack --env.dev",
    "dev:server": "webpack-dev-server --env.dev",
    "test": "jest",
    "test:watch": "hest --watch --coverage",
    "precommit": "npm-run-all validate",
    "validate": "npm-run-all --parallel lint test"
  },
  "dependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "img-loader": "^2.0.0",
    "jest": "^19.0.2",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
